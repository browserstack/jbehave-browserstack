{
  "server": "hub.browserstack.com",
  "userName": "BROWSERSTACK_USERNAME",
  "accessKey": "BROWSERSTACK_ACCESS_KEY",
  "capabilities": {
    "bstack:options": {
      "buildName": "browserstack-build-1",
      "sessionName": "BStack parallel jbehave",
      "debug": true
    }
  },
  "environments": [
    {
      "browserName": "chrome",
      "bstack:options": {
        "os": "OS X",
        "osVersion": "Big Sur"
      }
    },
    {
      "browserName": "firefox",
      "bstack:options": {
        "os": "Windows",
        "osVersion": "10"
      }
    },
    {
      "browserName": "safari",
      "bstack:options": {
        "os": "OS X",
        "osVersion": "Monterey"
      }
    }
  ]
}
